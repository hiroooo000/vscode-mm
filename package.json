{
  "name": "vscode-mm",
  "displayName": "Mind Map",
  "description": "Mind Map extension for VS Code",
  "publisher": "hiroooo000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiroooo000/vscode-mm"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCustomEditor:vscode-mm.mindmap"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-mm.mindmap",
        "displayName": "Mind Map",
        "selector": [
          {
            "filenamePattern": "*.mm"
          },
          {
            "filenamePattern": "*.mindmap"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:webview": "mocha media/main.test.js",
    "package": "vsce package",
    "build": "npm run compile && npm run package"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/ui": "^4.0.16",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "jsdom": "^27.4.0",
    "jsdom-global": "^3.0.2",
    "linkedom": "^0.18.12",
    "mocha": "^10.1.0",
    "sinon": "^21.0.1",
    "typescript": "^4.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "mind-elixir": "^5.3.8"
  }
}