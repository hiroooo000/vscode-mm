{
  "name": "vscode-mm",
  "displayName": "Mind Map",
  "description": "Mind Map extension for VS Code",
  "publisher": "hiroooo000",
  "icon": "media/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiroooo000/vscode-mm"
  },
  "homepage": "https://github.com/hiroooo000/vscode-mm/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/hiroooo000/vscode-mm/issues"
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.74.0"
  },
  "keywords": [
    "mindmap",
    "mind-elixir",
    "visualization",
    "planner",
    "idea",
    "brainstorm"
  ],
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCustomEditor:vscode-mm.mindmap"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-mm.mindmap",
        "displayName": "Mind Map",
        "selector": [
          {
            "filenamePattern": "*.mm"
          },
          {
            "filenamePattern": "*.mindmap"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js",
    "test:devcontainer": "xvfb-run -a npm test",
    "test:webview": "mocha media/main.test.js",
    "test:all": "npm run test && npm run test:webview",
    "test:all:devcontainer": "npm run test:devcontainer && npm run test:webview",
    "package": "vsce package",
    "build": "npm run compile && npm run package"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^4.0.16",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.0.0",
    "glob": "^11.0.0",
    "jsdom": "^27.4.0",
    "jsdom-global": "^3.0.2",
    "linkedom": "^0.18.12",
    "mocha": "^11.0.0",
    "sinon": "^21.0.1",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "mind-elixir": "^5.3.8"
  }
}